{
  "name": "rahatbin/pay",
  "description": "Modular multi-gateway payment system for Laravel/PHP",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "alirezasadeghian79/rahatpay": "master"
  },
  "autoload": {
    "psr-4": {
      "rahatPay\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "rahatPay\\Providers\\PaymentServiceProvider"
      ]
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/alirezasadeghian79/rahatPay"
    }
  ],
  "minimum-stability": "stable"
}