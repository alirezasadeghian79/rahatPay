{
  "name": "alirezasadeghian79/rahatpay",
  "description": "Modular multi-gateway payment system for Laravel/PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Alireza Sadeghian",
      "email": "alirezasadeghian52@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "rahatPay\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "rahatPay\\Providers\\PaymentServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}